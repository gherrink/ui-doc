{
  "name": "@ui-doc/node",
  "description": "Node implementations for UI-Doc.",
  "version": "0.1.1",
  "license": "MIT",
  "keywords": [
    "ui-doc",
    "styleguide",
    "ui-kit",
    "documentation",
    "generation",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gherrink/ui-doc",
    "directory": "packages/node"
  },
  "author": "Maurice Busch",
  "homepage": "https://github.com/gherrink/ui-doc/tree/master/packages/node#readme",
  "bugs": "https://github.com/gherrink/ui-doc/issues",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prebuild": "rm -rf dist dist",
    "prepare": "if [ ! -d 'dist' ]; then pnpm build; fi",
    "prerelease": "pnpm build",
    "release": "pnpm --workspace-root package:release $(pwd)",
    "release:dry": "pnpm --workspace-root package:release-dry $(pwd)",
    "test": "jest"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@ui-doc/core": "workspace:>=0.1.0",
    "node": "^16.0.0||^18.0.0||^20.0.0||^22.0.0"
  },
  "dependencies": {
    "picomatch": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/picomatch": "^2.3.3",
    "@ui-doc/core": "workspace:^"
  }
}
